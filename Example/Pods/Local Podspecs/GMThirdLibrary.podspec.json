{
  "name": "GMThirdLibrary",
  "version": "0.3.0",
  "summary": "三方的依赖库",
  "description": "三方的依赖库\n1.钥匙串保存库\n2.依赖注入 objection\n3.微信支付 分享 支持 1.8.6",
  "homepage": "https://github.com/ioszhanghui/GMThirdLibrary.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ioszhanghui@163.com": "yomingyo@gmail.com"
  },
  "source": {
    "git": "https://github.com/ioszhanghui/GMThirdLibrary.git",
    "tag": "0.3.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": [
    "GMThirdLibrary/Classes/GMAlipaySDK/**/*",
    "GMThirdLibrary/Classes/GMBaseSDK/**/*",
    "GMThirdLibrary/Classes/GMUMSDK/**/*",
    "GMThirdLibrary/Classes/IMSDK_ios/**/*",
    "GMThirdLibrary/Classes/objection/**/*"
  ],
  "static_framework": true,
  "subspecs": [
    {
      "name": "GMBaseSDK",
      "subspecs": [
        {
          "name": "GMTencentOpenApiSDK",
          "ios": {
            "vendored_frameworks": "GMThirdLibrary/Classes/GMBaseSDK/GMTencentOpenApiSDK/*.framework"
          },
          "frameworks": [
            "Security",
            "SystemConfiguration",
            "CoreGraphics",
            "CoreTelephony"
          ],
          "libraries": [
            "iconv",
            "z",
            "c++",
            "sqlite3"
          ]
        },
        {
          "name": "GMWechatSDK",
          "source_files": "GMThirdLibrary/Classes/GMBaseSDK/GMWechatSDK/*.h",
          "ios": {
            "vendored_libraries": "GMThirdLibrary/Classes/GMBaseSDK/GMWechatSDK/*.a"
          },
          "frameworks": [
            "Security",
            "SystemConfiguration",
            "CoreGraphics",
            "CoreTelephony",
            "CFNetwork",
            "CoreGraphics"
          ],
          "libraries": [
            "iconv",
            "z",
            "c++",
            "sqlite3"
          ]
        },
        {
          "name": "GMWeibo_SDK",
          "source_files": "GMThirdLibrary/Classes/GMBaseSDK/GMWeibo_SDK/*.h",
          "ios": {
            "vendored_libraries": "GMThirdLibrary/Classes/GMBaseSDK/GMWeibo_SDK/*.a"
          },
          "resources": "GMThirdLibrary/Assets/GMWeibo_SDK/*.bundle",
          "frameworks": [
            "Security",
            "SystemConfiguration",
            "CoreGraphics",
            "CoreTelephony",
            "CFNetwork",
            "CoreGraphics",
            "QuartzCore",
            "ImageIO",
            "CoreText",
            "Photos"
          ],
          "libraries": [
            "iconv",
            "z",
            "c++",
            "sqlite3"
          ]
        }
      ]
    },
    {
      "name": "GMUMSDK",
      "dependencies": {
        "GMThirdLibrary/GMBaseSDK": [

        ],
        "UMCCommon": [

        ]
      },
      "subspecs": [
        {
          "name": "Core",
          "ios": {
            "vendored_frameworks": "GMThirdLibrary/Classes/GMUMSDK/Core/*.framework"
          }
        },
        {
          "name": "UMSocialUI",
          "ios": {
            "vendored_frameworks": "GMThirdLibrary/Classes/GMUMSDK/UMSocialUI/*.framework",
            "resources": "GMThirdLibrary/Assets/UMBundle/*.bundle"
          },
          "dependencies": {
            "GMThirdLibrary/GMUMSDK/UMShare": [

            ],
            "GMThirdLibrary/GMUMSDK/Core": [

            ]
          }
        },
        {
          "name": "UMShare",
