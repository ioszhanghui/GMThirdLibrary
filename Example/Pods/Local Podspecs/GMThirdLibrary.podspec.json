                "vendored_libraries": "GMThirdLibrary/Classes/GMUMSDK/UMShare/QQ/*.a"
              },
              "dependencies": {
                "GMThirdLibrary/GMBaseSDK/GMTencentOpenApiSDK": [

                ],
                "GMThirdLibrary/GMUMSDK/Core": [

                ],
                "UMCCommon": [

                ]
              }
            },
            {
              "name": "Sina",
              "source_files": "GMThirdLibrary/Classes/GMUMSDK/UMShare/Sina/*.h",
              "ios": {
                "vendored_libraries": "GMThirdLibrary/Classes/GMUMSDK/UMShare/Sina/*.a"
              },
              "dependencies": {
                "GMThirdLibrary/GMBaseSDK/GMWeibo_SDK": [

                ],
                "GMThirdLibrary/GMUMSDK/Core": [

                ],
                "UMCCommon": [

                ]
              }
            },
            {
              "name": "WeChat",
              "source_files": "GMThirdLibrary/Classes/GMUMSDK/UMShare/WeChat/*.h",
              "ios": {
                "vendored_libraries": "GMThirdLibrary/Classes/GMUMSDK/UMShare/WeChat/*.a"
              },
              "dependencies": {
                "GMThirdLibrary/GMBaseSDK/GMWechatSDK": [

                ],
                "GMThirdLibrary/GMUMSDK/Core": [

                ],
                "UMCCommon": [

                ]
              }
            }
          ]
        }
      ]
    },
    {
      "name": "IMSDK_ios",
      "ios": {
        "vendored_libraries": "GMThirdLibrary/Classes/IMSDK_ios/*.a",
        "resources": "GMThirdLibrary/Assets/GMIMSDKRelease/*.bundle"
      },
      "source_files": "GMThirdLibrary/Classes/IMSDK_ios/**/*.h",
      "frameworks": [
        "SystemConfiguration",
        "UIKit",
        "Foundation"
      ],
      "libraries": "sqlite3",
      "dependencies": {
        "Reachability": [

        ],
        "AFNetworking": [

        ],
        "FMDB": [

        ],
        "SSZipArchive": [

        ],
        "SAMKeychain": [

        ]
      }
    },
    {
      "name": "GMAlipaySDK",
      "ios": {
        "vendored_frameworks": "GMThirdLibrary/Classes/GMAlipaySDK/*.framework",
        "vendored_libraries": "GMThirdLibrary/Classes/GMAlipaySDK/*.a"
      },
      "resources": "GMThirdLibrary/Assets/GMAlipayBundle/*.bundle",
      "libraries": [
        "stdc++",
        "iconv",
        "sqlite3",
        "z",
        "icucore",
        "c++"
      ],
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreTelephony",
        "CoreGraphics",
        "SystemConfiguration",
        "Security",
        "CoreMotion",
        "CoreGraphics",
        "CFNetwork"
      ]
    },
    {
      "name": "GMkeyChain",
      "source_files": "GMThirdLibrary/Classes/keyChain/**/*",
      "frameworks": "Security"
    },
    {
      "name": "GMObjection",
      "source_files": "GMThirdLibrary/Classes/objection/**/*",
      "frameworks": "WebKit"
    }
  ]
}
